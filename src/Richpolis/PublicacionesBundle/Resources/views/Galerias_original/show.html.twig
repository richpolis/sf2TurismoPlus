{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Galeria</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Titulo</th>
                <td>{{ entity.titulo }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Thumbnail</th>
                <td>{{ entity.thumbnail }}</td>
            </tr>
            <tr>
                <th>Archivo</th>
                <td>{{ entity.archivo }}</td>
            </tr>
            <tr>
                <th>Tipoarchivo</th>
                <td>{{ entity.tipoArchivo }}</td>
            </tr>
            <tr>
                <th>Position</th>
                <td>{{ entity.position }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{{ entity.isActive }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('galerias') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('galerias_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
